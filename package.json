  "devDependencies": {
    "@types/jest": "^29.0.3",
    "@types/node": "^18.7.23",
    "jest": "^29.0.3",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
